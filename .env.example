# Database connection (use host.docker.internal instead of localhost for Docker)
DB_CONNECTION_STRING=Host=host.docker.internal;Port=5432;Database=findhoneypos;Username=your_user;Password=your_password

# JWT secret (must be at least 32 characters)
JWT_SECRET=your_jwt_secret_here_must_be_at_least_32_chars

# CORS allowed origins (comma-separated)
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:5175
